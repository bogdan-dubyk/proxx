#!/usr/bin/env php
<?php

declare(strict_types=1);

require __DIR__.'/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use BohdanDubyk\Proxx\Commands\Play;

$application = new Application();

$play = new Play();
$application->add($play);
$application->setDefaultCommand($play->getName());
$application->run();
